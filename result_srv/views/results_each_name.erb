
<h1>Results of <%=h name %></h1>

<table>
  <% vs = MEM_DB[name] %>
  <% vs.reverse_each{|(t, opts)| %>
    <% next unless t %>
    <tr>
      <td> <a href='/results/<%=h name %>'><%=h name %></a></td>
      <td> <a href='/results/<%=h name %>/<%=h t %>'><%= Time.at(t) %></a></td>
      <td>
      <% if opts[:detail_link] %>
	<a href='<%=opts[:detail_link]%>'><%=h opts[:result] %></a>
      <% else %>
	<%=h opts[:result] %>
      <% end %>
      </td>
      <td>
    </tr>
  <% } %>
</table>
